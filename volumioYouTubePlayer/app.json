{
  "expo": {
    "name": "Volumio",
    "slug": "Volumio",
    "platforms": ["android"],
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "backgroundColor": "#75a3ff"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#75a3ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.Arciiix.volumioYouTubePlayer",
      "backgroundColor": "#75a3ff"
    },
    "assetBundlePatterns": ["**/*"]
  }
}
